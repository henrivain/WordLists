<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModel="clr-namespace:WordListsViewModels.Interfaces;assembly=WordListsViewModels"
             x:Class="WordListsUI.WordDataPages.ImageRecognisionPage.ImageRecognisionPage"
             Title="ImageRecognisionPage"
             x:DataType="{x:Type viewModel:IImageRecognisionViewModel}"
             >
    <ScrollView>
        <VerticalStackLayout>
            <Button Text="press me!" Clicked="Button_Clicked"/>
            <Label Text="{Binding Text}"
                   VerticalOptions="Center"
                   HorizontalOptions="Center" />
        </VerticalStackLayout>
        
    </ScrollView>
</ContentPage>